<?xml version="1.0" encoding="UTF-8"?>
<project name="module_man10homeplugin" default="compile.module.man10homeplugin">
  <dirname property="module.man10homeplugin.basedir" file="${ant.file.module_man10homeplugin}"/>
  
  <property name="module.jdk.home.man10homeplugin" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.man10homeplugin" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.man10homeplugin" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.man10homeplugin" value="-encoding UTF-8 -source 8 -target 1.8 ${compiler.args}"/>
  
  <property name="man10homeplugin.output.dir" value="${module.man10homeplugin.basedir}/target/classes"/>
  <property name="man10homeplugin.testoutput.dir" value="${module.man10homeplugin.basedir}/target/test-classes"/>
  
  <path id="man10homeplugin.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="man10homeplugin.module.production.classpath">
    <path refid="${module.jdk.classpath.man10homeplugin}"/>
    <path refid="library.maven:_org.spigotmc:spigot-api:1.12-r0.1-snapshot.classpath"/>
    <path refid="library.maven:_commons-lang:commons-lang:2.6.classpath"/>
    <path refid="library.maven:_com.googlecode.json-simple:json-simple:1.1.1.classpath"/>
    <path refid="library.maven:_junit:junit:4.10.classpath"/>
    <path refid="library.maven:_org.hamcrest:hamcrest-core:1.1.classpath"/>
    <path refid="library.maven:_com.google.guava:guava:21.0.classpath"/>
    <path refid="library.maven:_com.google.code.gson:gson:2.8.0.classpath"/>
    <path refid="library.maven:_org.yaml:snakeyaml:1.18.classpath"/>
    <path refid="library.maven:_net.md-5:bungeecord-chat:1.12-snapshot.classpath"/>
    <path refid="library.vault.classpath"/>
  </path>
  
  <path id="man10homeplugin.runtime.production.module.classpath">
    <pathelement location="${man10homeplugin.output.dir}"/>
    <path refid="library.vault.classpath"/>
  </path>
  
  <path id="man10homeplugin.module.classpath">
    <path refid="${module.jdk.classpath.man10homeplugin}"/>
    <pathelement location="${man10homeplugin.output.dir}"/>
    <path refid="library.maven:_org.spigotmc:spigot-api:1.12-r0.1-snapshot.classpath"/>
    <path refid="library.maven:_commons-lang:commons-lang:2.6.classpath"/>
    <path refid="library.maven:_com.googlecode.json-simple:json-simple:1.1.1.classpath"/>
    <path refid="library.maven:_junit:junit:4.10.classpath"/>
    <path refid="library.maven:_org.hamcrest:hamcrest-core:1.1.classpath"/>
    <path refid="library.maven:_com.google.guava:guava:21.0.classpath"/>
    <path refid="library.maven:_com.google.code.gson:gson:2.8.0.classpath"/>
    <path refid="library.maven:_org.yaml:snakeyaml:1.18.classpath"/>
    <path refid="library.maven:_net.md-5:bungeecord-chat:1.12-snapshot.classpath"/>
    <path refid="library.vault.classpath"/>
  </path>
  
  <path id="man10homeplugin.runtime.module.classpath">
    <pathelement location="${man10homeplugin.testoutput.dir}"/>
    <pathelement location="${man10homeplugin.output.dir}"/>
    <path refid="library.maven:_org.spigotmc:spigot-api:1.12-r0.1-snapshot.classpath"/>
    <path refid="library.maven:_commons-lang:commons-lang:2.6.classpath"/>
    <path refid="library.maven:_com.googlecode.json-simple:json-simple:1.1.1.classpath"/>
    <path refid="library.maven:_junit:junit:4.10.classpath"/>
    <path refid="library.maven:_org.hamcrest:hamcrest-core:1.1.classpath"/>
    <path refid="library.maven:_com.google.guava:guava:21.0.classpath"/>
    <path refid="library.maven:_com.google.code.gson:gson:2.8.0.classpath"/>
    <path refid="library.maven:_org.yaml:snakeyaml:1.18.classpath"/>
    <path refid="library.maven:_net.md-5:bungeecord-chat:1.12-snapshot.classpath"/>
    <path refid="library.vault.classpath"/>
  </path>
  
  
  <patternset id="excluded.from.module.man10homeplugin">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.man10homeplugin">
    <patternset refid="excluded.from.module.man10homeplugin"/>
  </patternset>
  
  <path id="man10homeplugin.module.sourcepath">
    <dirset dir="${module.man10homeplugin.basedir}">
      <include name="src/main/java"/>
      <include name="src/main/resources"/>
    </dirset>
  </path>
  
  <path id="man10homeplugin.module.test.sourcepath">
    <dirset dir="${module.man10homeplugin.basedir}">
      <include name="src/test/java"/>
      <include name="src/test/resources"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.man10homeplugin" depends="compile.module.man10homeplugin.production,compile.module.man10homeplugin.tests" description="Compile module Man10HomePlugin"/>
  
  <target name="compile.module.man10homeplugin.production" depends="register.custom.compilers" description="Compile module Man10HomePlugin; production classes">
    <mkdir dir="${man10homeplugin.output.dir}"/>
    <javac2 destdir="${man10homeplugin.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.man10homeplugin}/javac">
      <compilerarg line="${compiler.args.man10homeplugin}"/>
      <bootclasspath refid="man10homeplugin.module.bootclasspath"/>
      <classpath refid="man10homeplugin.module.production.classpath"/>
      <src refid="man10homeplugin.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.man10homeplugin"/>
    </javac2>
    
    <copy todir="${man10homeplugin.output.dir}">
      <fileset dir="${module.man10homeplugin.basedir}/src/main/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.man10homeplugin.basedir}/src/main/resources">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.man10homeplugin.tests" depends="register.custom.compilers,compile.module.man10homeplugin.production" description="compile module Man10HomePlugin; test classes" unless="skip.tests">
    <mkdir dir="${man10homeplugin.testoutput.dir}"/>
    <javac2 destdir="${man10homeplugin.testoutput.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.man10homeplugin}/javac">
      <compilerarg line="${compiler.args.man10homeplugin}"/>
      <bootclasspath refid="man10homeplugin.module.bootclasspath"/>
      <classpath refid="man10homeplugin.module.classpath"/>
      <src refid="man10homeplugin.module.test.sourcepath"/>
      <patternset refid="excluded.from.compilation.man10homeplugin"/>
    </javac2>
    
    <copy todir="${man10homeplugin.testoutput.dir}">
      <fileset dir="${module.man10homeplugin.basedir}/src/test/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.man10homeplugin.basedir}/src/test/resources">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="clean.module.man10homeplugin" description="cleanup module">
    <delete dir="${man10homeplugin.output.dir}"/>
    <delete dir="${man10homeplugin.testoutput.dir}"/>
  </target>
</project>